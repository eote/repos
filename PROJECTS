localroot = /myplace/repos/
GitHostsPublic = (
	source = local.git/
	mirrors[0] = assembla.git/
	mirrors[1] = github.git/
	mirrors[2] = gitorious.git/
	mirrors[3] = google.git/
	mirrors[4] = sf-zrepos.git/
)

GitHostsPublic2 = (
	source = local.git/
